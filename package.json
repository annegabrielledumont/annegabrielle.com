  "scripts": {
    "dev": "webpack-dev-server --open",
    "dev-css": "postcss src/css/styles.css -o dist/css/styles.css",
    "build": "rm -rf dist && NODE_ENV=production webpack --progress --hide-modules",
    "svgo": "svgo -f --pretty ./src -r"
