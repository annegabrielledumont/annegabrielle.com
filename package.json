  "browserslist": [
    "defaults"
  ],
  "eslintConfig": {
    "extends": [
      "prettier-standard"
    ],
    "plugins": [
      "html"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
